dy.classList.add(Theme.DARK);\n        refs.themesToggle.checked = true;\n    }\n\n    let menu = document.querySelector('.js-menu');\n\n    function createMenu (arr) {\n        const menuElements = arr.map(it => menuTemplate(it)).join('');\n        menu.insertAdjacentHTML('beforeend', menuElements);\n    };\n\n    createMenu(menuElement);"},"lineCount":null}},"error":null,"hash":"a05ac5058c9ccbb37a325ed0be3db515","cacheData":{"env":{}}}